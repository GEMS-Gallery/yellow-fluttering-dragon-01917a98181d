type Result = 
 variant {
   err: text;
   ok: text;
 };
type Listing = 
 record {
   categoryId: text;
   description: text;
   id: text;
   price: opt float64;
   title: text;
 };
type Category = 
 record {
   icon: text;
   id: text;
   name: text;
 };
service : {
  createListing: (text, text, text, opt text) -> (Result);
  getCategories: () -> (vec Category) query;
  getListings: (text) -> (vec Listing) query;
}
